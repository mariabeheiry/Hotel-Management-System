@model IEnumerable<Hotel_Management_System.Models.Guest>

@foreach (var item in Model)
{
        <tr>
            <td>@item.Name</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.GuestID" class="btn btn-sm btn-primary">Edit</a>
                <a asp-action="Details" asp-route-id="@item.GuestID" class="btn btn-sm btn-info">Details</a>
                <a asp-action="Delete" asp-route-id="@item.GuestID" class="btn btn-sm btn-danger">Delete</a>
            </td>
        </tr>
}
